{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 10"
          ]
        }
      }
    ],
    "stage-3",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "syntax-dynamic-import",
    ["import", { "libraryName": "antd-mobile",  "style": true }],
    ["transform-class-properties", { "spec": true }]
  ]
}